{
  "name": "@dkmassive/proto",
  "description": "Command line tool for theming with the Drupal Protoype starter theme.",
  "version": "0.1.0",
  "author": "Devin Kendall",
  "bin": {
    "proto": "bin/run"
  },
  "bugs": {
    "url": "https://github.com/dk-massive/proto/issues"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "create-file": "^1.0.1",
    "file-system": "^2.2.2",
    "inquirer": "^7.3.3",
    "mkdirp": "^1.0.4",
    "param-case": "^3.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/dk-massive/proto",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "proto",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dk-massive/proto.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
